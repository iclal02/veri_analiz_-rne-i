      "93               no             yes        3       no   semi-furnished   \n",
      "225              no             yes        3       no   semi-furnished   \n",
      "247              no              no        3       no      unfurnished   \n",
      "299              no              no        3       no        furnished   \n",
      "304              no              no        3       no   semi-furnished   \n",
      "323              no              no        3       no   semi-furnished   \n",
      "331              no              no        3       no        furnished   \n",
      "401              no              no        3      yes      unfurnished   \n",
      "472              no              no        3       no   semi-furnished   \n",
      "\n",
      "    aykiri_mi  aykiri_sayisi  \n",
      "1      Aykırı              4  \n",
      "3      Normal              2  \n",
      "47     Normal              2  \n",
      "93     Normal              1  \n",
      "225    Normal              1  \n",
      "247    Normal              2  \n",
      "299    Normal              1  \n",
      "304    Normal              1  \n",
      "323    Normal              1  \n",
      "331    Normal              1  \n",
      "401    Normal              1  \n",
      "472    Normal              1  \n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# Sayısal sütunları seçtik\n",
    "numeric_cols = df.select_dtypes(include=['int64', 'float64']).columns\n",
    "df['aykiri_mi'] = 'Normal'\n",
    "df['aykiri_sayisi'] = 0\n",
    "# Eşik değeri belirleyelim (örneğin, 2 sütundan fazla aykırı ise aykırı olarak sınıflandıralım)\n",
    "esik_deger = 3\n",
    "\n",
    "for column in numeric_cols:\n",
    "    Q1 = df[column].quantile(0.25)\n",
    "    Q3 = df[column].quantile(0.75)\n",
    "    IQR = Q3 - Q1\n",
    "\n",
    "    lower_bound = Q1 - 1.5 * IQR\n",
    "    upper_bound = Q3 + 1.5 * IQR\n",
    "\n",
    "    # Aykırı değerleri belirleyelim ve 'aykiri_sayisi' sütununu artıralım\n",
    "    df.loc[(df[column] < lower_bound) | (df[column] > upper_bound), 'aykiri_sayisi'] += 1\n",
    "\n",
    "    # 'aykiri_sayisi' sütunu 2'yi geçen satırları 'aykiri_mi' olarak işaretleyelim\n",
    "    df.loc[df['aykiri_sayisi'] >= esik_deger, 'aykiri_mi'] = 'Aykırı'\n",
    "\n",
    "    # Aykırı değerleri sütunlar bazında incelemek için ek bilgi çıktısı\n",
    "for column in numeric_cols:\n",
    "    outliers = df[(df[column] < lower_bound) | (df[column] > upper_bound)]\n",
    "    print(f\"Aykırı değerler {column} sütununda:\")\n",
    "    print(outliers)"
   ]
  },
